{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "name": "Anime Tinder Design System",
  "version": "1.0.0",

  "colors": {
    "light": {
      "background": "oklch(1 0 0)",
      "foreground": "oklch(0.145 0 0)",
      "card": "oklch(1 0 0)",
      "cardForeground": "oklch(0.145 0 0)",
      "popover": "oklch(1 0 0)",
      "popoverForeground": "oklch(0.145 0 0)",
      "primary": "oklch(0.205 0 0)",
      "primaryForeground": "oklch(0.985 0 0)",
      "secondary": "oklch(0.97 0 0)",
      "secondaryForeground": "oklch(0.205 0 0)",
      "muted": "oklch(0.97 0 0)",
      "mutedForeground": "oklch(0.556 0 0)",
      "accent": "oklch(0.97 0 0)",
      "accentForeground": "oklch(0.205 0 0)",
      "destructive": "oklch(0.577 0.245 27.325)",
      "border": "oklch(0.922 0 0)",
      "input": "oklch(0.922 0 0)",
      "ring": "oklch(0.708 0 0)"
    },
    "dark": {
      "background": "oklch(0.145 0 0)",
      "foreground": "oklch(0.985 0 0)",
      "card": "oklch(0.205 0 0)",
      "cardForeground": "oklch(0.985 0 0)",
      "popover": "oklch(0.205 0 0)",
      "popoverForeground": "oklch(0.985 0 0)",
      "primary": "oklch(0.922 0 0)",
      "primaryForeground": "oklch(0.205 0 0)",
      "secondary": "oklch(0.269 0 0)",
      "secondaryForeground": "oklch(0.985 0 0)",
      "muted": "oklch(0.269 0 0)",
      "mutedForeground": "oklch(0.708 0 0)",
      "accent": "oklch(0.269 0 0)",
      "accentForeground": "oklch(0.985 0 0)",
      "destructive": "oklch(0.704 0.191 22.216)",
      "border": "oklch(1 0 0 / 10%)",
      "input": "oklch(1 0 0 / 15%)",
      "ring": "oklch(0.556 0 0)"
    },
    "brand": {
      "purple": {
        "50": "#faf5ff",
        "100": "#f3e8ff",
        "200": "#e9d5ff",
        "300": "#d8b4fe",
        "400": "#c084fc",
        "500": "#a855f7",
        "600": "#9333ea",
        "700": "#7e22ce",
        "800": "#6b21a8",
        "900": "#581c87"
      },
      "pink": {
        "50": "#fdf2f8",
        "100": "#fce7f3",
        "200": "#fbcfe8",
        "300": "#f9a8d4",
        "400": "#f472b6",
        "500": "#ec4899",
        "600": "#db2777",
        "700": "#be185d",
        "800": "#9d174d",
        "900": "#831843"
      }
    },
    "chart": {
      "1": "oklch(0.646 0.222 41.116)",
      "2": "oklch(0.6 0.118 184.704)",
      "3": "oklch(0.398 0.07 227.392)",
      "4": "oklch(0.828 0.189 84.429)",
      "5": "oklch(0.769 0.188 70.08)"
    },
    "sidebar": {
      "background": "oklch(0.985 0 0)",
      "foreground": "oklch(0.145 0 0)",
      "primary": "oklch(0.205 0 0)",
      "primaryForeground": "oklch(0.985 0 0)",
      "accent": "oklch(0.97 0 0)",
      "accentForeground": "oklch(0.205 0 0)",
      "border": "oklch(0.922 0 0)",
      "ring": "oklch(0.708 0 0)"
    }
  },

  "typography": {
    "fontFamily": {
      "sans": "ui-sans-serif, system-ui, sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji'"
    },
    "fontSize": {
      "xs": { "size": "0.75rem", "lineHeight": "1rem" },
      "sm": { "size": "0.875rem", "lineHeight": "1.25rem" },
      "base": { "size": "1rem", "lineHeight": "1.5rem" },
      "lg": { "size": "1.125rem", "lineHeight": "1.75rem" },
      "xl": { "size": "1.25rem", "lineHeight": "1.75rem" },
      "2xl": { "size": "1.5rem", "lineHeight": "2rem" },
      "3xl": { "size": "1.875rem", "lineHeight": "2.25rem" },
      "4xl": { "size": "2.25rem", "lineHeight": "2.5rem" },
      "5xl": { "size": "3rem", "lineHeight": "1" },
      "6xl": { "size": "3.75rem", "lineHeight": "1" },
      "7xl": { "size": "4.5rem", "lineHeight": "1" }
    },
    "fontWeight": {
      "normal": "400",
      "medium": "500",
      "semibold": "600",
      "bold": "700"
    },
    "lineHeight": {
      "none": "1",
      "tight": "1.25",
      "snug": "1.375",
      "normal": "1.5",
      "relaxed": "1.625",
      "loose": "2"
    }
  },

  "spacing": {
    "0": "0",
    "0.5": "0.125rem",
    "1": "0.25rem",
    "1.5": "0.375rem",
    "2": "0.5rem",
    "2.5": "0.625rem",
    "3": "0.75rem",
    "3.5": "0.875rem",
    "4": "1rem",
    "5": "1.25rem",
    "6": "1.5rem",
    "7": "1.75rem",
    "8": "2rem",
    "9": "2.25rem",
    "10": "2.5rem",
    "11": "2.75rem",
    "12": "3rem",
    "14": "3.5rem",
    "16": "4rem",
    "20": "5rem",
    "24": "6rem",
    "28": "7rem",
    "32": "8rem"
  },

  "borderRadius": {
    "none": "0",
    "sm": "calc(0.625rem - 4px)",
    "md": "calc(0.625rem - 2px)",
    "lg": "0.625rem",
    "xl": "calc(0.625rem + 4px)",
    "2xl": "calc(0.625rem + 8px)",
    "3xl": "calc(0.625rem + 12px)",
    "4xl": "calc(0.625rem + 16px)",
    "full": "9999px",
    "base": "0.625rem"
  },

  "shadows": {
    "xs": "0 1px 2px 0 rgb(0 0 0 / 0.05)",
    "sm": "0 1px 3px 0 rgb(0 0 0 / 0.1), 0 1px 2px -1px rgb(0 0 0 / 0.1)",
    "md": "0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1)",
    "lg": "0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1)",
    "xl": "0 20px 25px -5px rgb(0 0 0 / 0.1), 0 8px 10px -6px rgb(0 0 0 / 0.1)",
    "2xl": "0 25px 50px -12px rgb(0 0 0 / 0.25)",
    "inner": "inset 0 2px 4px 0 rgb(0 0 0 / 0.05)",
    "none": "none"
  },

  "animations": {
    "accordionDown": {
      "keyframes": "from { height: 0 } to { height: var(--radix-accordion-content-height) }",
      "duration": "0.2s",
      "timing": "ease-out"
    },
    "accordionUp": {
      "keyframes": "from { height: var(--radix-accordion-content-height) } to { height: 0 }",
      "duration": "0.2s",
      "timing": "ease-out"
    },
    "countdown": {
      "keyframes": "from { stroke-dashoffset: 0 } to { stroke-dashoffset: var(--circumference) }",
      "duration": "3s",
      "timing": "linear"
    },
    "fadeIn": { "class": "animate-in fade-in", "duration": "1000ms" },
    "zoomIn": { "class": "animate-in zoom-in", "duration": "700ms" },
    "bounce": { "class": "animate-bounce" },
    "pulse": { "class": "animate-pulse" },
    "spin": { "class": "animate-spin" }
  },

  "components": {
    "button": {
      "base": "inline-flex items-center justify-center gap-2 whitespace-nowrap rounded-md text-sm font-medium transition-all disabled:pointer-events-none disabled:opacity-50",
      "variants": {
        "default": "bg-primary text-primary-foreground shadow-xs hover:bg-primary/90",
        "destructive": "bg-destructive text-white shadow-xs hover:bg-destructive/90",
        "outline": "border border-input bg-background shadow-xs hover:bg-accent hover:text-accent-foreground",
        "secondary": "bg-secondary text-secondary-foreground shadow-xs hover:bg-secondary/80",
        "ghost": "hover:bg-accent hover:text-accent-foreground",
        "link": "text-primary underline-offset-4 hover:underline"
      },
      "sizes": {
        "default": "h-9 px-4 py-2",
        "sm": "h-8 rounded-md px-3 text-xs",
        "lg": "h-10 rounded-md px-6 text-base",
        "icon": "size-9",
        "icon-sm": "size-8",
        "icon-lg": "size-10"
      }
    },
    "card": {
      "base": "bg-card text-card-foreground flex flex-col gap-6 rounded-xl border py-6 shadow-sm",
      "header": "flex flex-col gap-1.5 px-6",
      "title": "leading-none font-semibold",
      "description": "text-muted-foreground text-sm",
      "content": "px-6",
      "footer": "flex items-center px-6"
    },
    "input": {
      "base": "border-input bg-transparent file:text-foreground placeholder:text-muted-foreground flex h-9 w-full rounded-md border px-3 py-1 text-base shadow-xs transition-colors md:text-sm",
      "focus": "focus-visible:border-ring focus-visible:ring-ring/50 focus-visible:ring-[3px]",
      "invalid": "aria-invalid:ring-destructive/20 dark:aria-invalid:ring-destructive/40 aria-invalid:border-destructive"
    },
    "badge": {
      "base": "inline-flex items-center justify-center rounded-full border px-2 py-0.5 text-xs font-medium transition-colors",
      "variants": {
        "default": "border-transparent bg-primary text-primary-foreground",
        "secondary": "border-transparent bg-secondary text-secondary-foreground",
        "destructive": "border-transparent bg-destructive text-white",
        "outline": "text-foreground"
      }
    },
    "avatar": {
      "base": "relative flex size-8 shrink-0 overflow-hidden rounded-full",
      "image": "aspect-square size-full",
      "fallback": "bg-muted flex size-full items-center justify-center rounded-full"
    },
    "separator": {
      "base": "bg-border shrink-0",
      "horizontal": "h-px w-full",
      "vertical": "h-full w-px"
    },
    "dropdown": {
      "content": "bg-popover text-popover-foreground z-50 min-w-[8rem] overflow-hidden rounded-md border p-1 shadow-md",
      "item": "relative flex cursor-default select-none items-center gap-2 rounded-sm px-2 py-1.5 text-sm outline-none"
    }
  },

  "patterns": {
    "focus": {
      "ring": "focus-visible:border-ring focus-visible:ring-ring/50 focus-visible:ring-[3px]",
      "outline": "outline-ring/50"
    },
    "transitions": {
      "default": "transition-all duration-300",
      "colors": "transition-colors duration-200",
      "transform": "transition-transform duration-300"
    },
    "gradients": {
      "primaryButton": "from-purple-600 to-pink-600",
      "text": "from-purple-400 via-pink-400 to-purple-400",
      "backgroundPurple": "from-purple-500/20 to-transparent",
      "backgroundPink": "from-pink-500/20 to-transparent"
    },
    "hover": {
      "card": "hover:border-purple-500/30 hover:shadow-lg hover:shadow-purple-500/10",
      "scale": "hover:scale-105"
    },
    "iconSize": {
      "default": "size-4",
      "sm": "size-3",
      "lg": "size-5"
    }
  }
}
